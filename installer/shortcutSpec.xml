<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="VIGCoin" location="startMenu"/>
	<shortcut
		name="VIGCoin Server"
		target="$INSTALL_PATH\VIGCoin.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start VIGCoin server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="VIGCoin Wallet"
		target="http://localhost:8033/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch VIGCoin wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>